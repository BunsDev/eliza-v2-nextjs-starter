{
  "name": "eliza.how",
  "description": "Project demonstrating how to make a documentation website with elizaOS",
  "version": "1.0.0-beta.41",
  "type": "module",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@elizaos/cli": "1.0.0-beta.41",
    "@elizaos/core": "1.0.0-beta.41",
    "@elizaos/plugin-anthropic": "1.0.0-beta.41",
    "@elizaos/plugin-bootstrap": "1.0.0-beta.41",
    "@elizaos/plugin-openai": "1.0.0-beta.41",
    "@elizaos/plugin-sql": "1.0.0-beta.41",
    "@elizaos/plugin-telegram": "1.0.0-beta.41",
    "concurrently": "^9.1.2",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "tsup": "8.4.0",
    "prettier": "3.5.3",
    "vitest": "2.1.5",
    "@vitest/coverage-v8": "2.1.5"
  },
  "scripts": {
    "postinstall": "cd client && bun install",
    "start": "concurrently \"elizaos start\" \"cd client && bun run start\"",
    "dev": "concurrently \"elizaos dev\" \"cd client && bun run dev\"",
    "build": "tsup",
    "lint": "prettier --write ./src",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src"
  }
}
